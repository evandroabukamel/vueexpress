{
  "name": "mevn-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "build": "babel . -d dist --presets es2015,stage-2 --ignore spec.js,node_modules"
  },
  "dependencies": {
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "^4.16.3",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "mongoose": "^5.3.0",
    "morgan": "^1.9.1",
    "serve-favicon": "^2.5.0",
    "server": "^1.0.18",
    "stripe": "^6.12.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-plugin-import": "^2.14.0",
    "faker": "^4.1.0",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
